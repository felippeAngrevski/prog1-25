
CREATE DATABASE IMOBILIARIA;
USE IMOBILIARIA;

CREATE TABLE `IMOVEL5` (
	`IMOVEL_ID` INT PRIMARY KEY auto_increment,
	`IMOVEL_NAME` CHAR(30) NOT NULL,
    `IMOVEL_DESC` CHAR (100) NOT NULL,
    `IMOVEL_NRO` INT NOT NULL,
    `IMOVEL_VALOR` FLOAT NOT NULL
);



